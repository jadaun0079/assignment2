
pipeline {
  agent any
  environment {
    BUCKET_NAME = 'your-bucket-name'
    REGION = 'your-region'
  }
  stages {
    stage('Clone') {
      steps {
        git 'https://github.com/your-username/static-website.git'
      }
    }
    stage('Deploy to S3') {
      steps {
        sh 'aws s3 sync . s3://$BUCKET_NAME --region $REGION --delete'
      }
    }
  }
}
